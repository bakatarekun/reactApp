{
  "name": "reactexpress",
  "version": "1.0.0",
  "description": "Boilerplate for React and Express",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server":"nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm  run server\" \"npm run client\"",
    "heroku-postbuild": "npm install --preifx client && npm run build --prefix"
    
  },
  "author": "Ken Y",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^4.1.2",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "nodemon": "^1.19.2"
  }
}
